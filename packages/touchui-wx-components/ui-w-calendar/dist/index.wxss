.ui-w-calendar {
  background-color: white;
}
.ui-w-calendar .calendar-title {
  height: 100rpx;
}
.ui-w-calendar .calendar-title .calendar-weekday {
  height: 100rpx;
  width: 14.2857%;
  float: left;
  line-height: 100rpx;
  text-align: center;
  font-size: 24rpx;
  font-weight: 500;
  color: #666;
}
.ui-w-calendar .calendar-wrap {
  height: 100rpx;
}
.ui-w-calendar .calendar-month-body .calendar-month-title {
  height: 100rpx;
  text-align: center;
  line-height: 100rpx;
}
.ui-w-calendar .calendar-month-body .calendar-day-wrap {
  height: 100rpx;
  width: 14.2857%;
  float: left;
  text-align: center;
  horizontal-align: middle;
  line-height: 100rpx;
}
.ui-w-calendar .calendar-month-body .calendar-day-wrap .calendar-day-cell {
  width: 64rpx;
  height: 64rpx;
  line-height: 64rpx;
  margin: 18rpx auto;
  border-radius: 32rpx;
}
.ui-w-calendar .calendar-month-body .calendar-day-wrap .is-this-month {
  color: black;
}
.ui-w-calendar .calendar-month-body .calendar-day-wrap .is-holiday {
  background-color: white;
  color: #666;
}
.ui-w-calendar .calendar-month-body .calendar-day-wrap .is-today {
  background-color: #808080;
  color: white;
}
.ui-w-calendar .calendar-month-body .calendar-day-wrap .is-selected {
  background-color: #00B886;
  color: white;
}
.ui-w-calendar .calendar-month-body .calendar-day-wrap .is-selected-left {
  background-color: #00B886;
  color: white;
}
.ui-w-calendar .calendar-month-body .calendar-day-wrap .is-selected-right {
  background-color: red;
  color: white;
}
.ui-w-calendar .calendar-month-body .calendar-day-wrap .is-selected-in-range {
  background-color: pink;
  color: white;
}
.ui-w-calendar .calendar-month-body .calendar-day-wrap .is-past-days {
  color: gray;
}
.ui-w-calendar .calendar-month-body .calendar-day-wrap .is-fore-days {
  color: gray;
}
