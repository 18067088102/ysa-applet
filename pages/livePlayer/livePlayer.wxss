.auth-btn {
  margin: 20rpx;
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 20rpx;
  color: #fff;
  font-size: 28rpx;
}

view.content {
  position: absolute;
  left: 0;
  right: 0;
  bottom: 0;
  height: auto;
  width: 100%;
}

.player-container {
