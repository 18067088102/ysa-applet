}

.list-supplier {
  font-size: 24rpx;
  color: #979797;
  margin-top: 8rpx;
  text-align: left;
}

.list-right {
  display: flex;
  flex-direction: column;
}

.right-top {
  display: flex;
  flex-direction: row;
  align-items: center;
}

.top-text {
  font-size: 26rpx;
