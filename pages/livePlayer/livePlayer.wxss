}

.select-date {
  display: flex;
  flex-direction: row;
  align-items: center;
}

.select-date .ui-icon {
  display: inline-block;
  transform: rotate(0deg);
  transition: all 0.3s ease-in;
}

.select-date .ui-icon.rotate {
  transform: rotate(180deg);
}

.select-text {
  margin-right: 10rpx;
  font-size: 26rpx;
  color: #333;
  font-weight: 500;
}

.calendartab {
  padding: 20rpx 0;
}

.ui-w-calendar {
  background-color: transparent;
  color: #fff;
}

.is-holiday {
  color: #fff !important;
}

.bg-blue {
  font-size: 28rpx;
  font-weight: 500;
  color: #666;
  background-color: #00b886;
  width: 100%;
  height: 200rpx;
  position: relative;
}

.VerticalNav.nav {
  width: 200rpx;
  white-space: initial;
  background-color: #f1f1f1;
}

.VerticalNav.nav .cu-item {
  width: 100%;
  text-align: center;
  background-color: #f1f1f1;
  margin: 0;
  border: none;
  height: 100rpx;
  line-height: 100rpx;
  position: relative;
}

.VerticalNav.nav .cu-item.cur {
  background-color: #fff;
}

.VerticalNav.nav .cu-item.cur::after {
  content: "";
  width: 8rpx;
  height: 30rpx;
  border-radius: 10rpx 0 0 10rpx;
  position: absolute;
  background-color: currentColor;
  top: 0;
  right: 0;
  bottom: 0;
  margin: auto;
}

.VerticalBox {
  display: flex;
}

.VerticalMain {
  background-color: #fff;
}

.item-view {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}

.item-img {
  width: 32rpx;
  height: 32rpx;
  margin-right: 10rpx;
}

.item-text {
  font-size: 28rpx;
  font-weight: 500;
}

.cu-bar {
  min-height: 70rpx;
}

.list-view {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: space-between;
  padding: 0 25rpx;
}

.sub-list {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  margin-top: 30rpx;
  width: 232rpx;
  height: 368rpx;
}

.name-view {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  width: 232rpx;
  height: 108rpx;
  box-shadow: 0px 2px 16px 0px rgba(0, 0, 0, 0.08);
  border-radius: 0px 0px 8px 8px;
}

.list-img {
  width: 232rpx;
  height: 260rpx;
}

.list-text {
  font-size: 26rpx;
  font-weight: 500;
  color: #333;
  text-align: center;
  margin-top: 11rpx;
}

.cu-modal.bottom-modal .cu-dialog {
  border-radius: 20rpx 20rpx 0px 0px;
  padding: 25rpx;
}

.modal-text {
  font-size: 30rpx;
  color: #333;
  font-weight: bold;
}

.arrow-img {
  width: 32rpx;
