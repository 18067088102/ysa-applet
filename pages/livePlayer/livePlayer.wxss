}

.date-head {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  width: 100%;
  padding: 25rpx 25rpx 0;
}

.episode-container {
  display: flex;
  flex-direction: row;
  height: 78rpx;
}

.left-episose {
  display: flex;
  flex-direction: row;
  align-items: center;
}

.left-episose text {
  font-size: 56rpx;
  font-weight: 500;
}

.index {
  font-size: 78rpx;
  line-height: 78rpx;
  font-weight: 500;
  margin-right: 10rpx;
  color: #333;
}

.episose-line {
  height: 78rpx;
  line-height: 78rpx;
  margin-right: 10rpx;
