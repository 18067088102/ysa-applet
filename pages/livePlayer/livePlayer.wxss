  font-weight: bold;
}

.head-level {
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-top: 15rpx;
  height: 37rpx;
}

.level-text1 {
  font-size: 26rpx;
  color: #666;
}

.level-text2 {
  width: 39rpx;
  height: 30rpx;
  border: 1rpx solid rgba(255, 112, 104, 1);
  font-size: 18rpx;
