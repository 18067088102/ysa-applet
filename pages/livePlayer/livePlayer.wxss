  color: #666;
  margin-top: 20rpx;
}

.right {
  display: flex;
  flex-direction: column;
}

.right text {
  font-size: 26rpx;
  color: #333;
  margin-top: 20rpx;
}

.activity {
  margin-top: 250rpx;
}

.activity .ui-icon {
  display: inline-block;
  transform: rotate(0deg);
  transition: all 0.3s ease-in;
}

.activity .ui-icon.rotate {
  transform: rotate(180deg);
}

